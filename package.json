{
	"name": "zed-sharing-node",
	"version": "1.0.0",
	"description": "API for P2P file sharing web application",
	"main": "dist/server.js",
	"scripts": {
		"dev": "tsc",
		"start": "tsc && node dist/server.js",
		"init": "npx sequelize-cli init",
		"prettier-format": "prettier --config .prettierrc",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix",
		"build": "npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all && tsc"
	},
	"keywords": [],
	"author": "Quavo",
	"license": "ISC",
	"dependencies": {
		"cors": "^2.8.5",
		"express": "^4.17.3",
		"mysql2": "^2.3.3",
		"sequelize": "^6.19.0",
		"sequelize-cli": "^6.4.1",
		"socket.io": "^4.4.1"
	},
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@typescript-eslint/eslint-plugin": "^5.20.0",
		"@typescript-eslint/parser": "^5.20.0",
		"eslint": "^8.13.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin": "^1.0.1",
		"husky": "^7.0.4",
		"prettier": "^2.6.2",
		"typescript": "^4.6.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run prettier-format && npm run lint"
		}
	}
}
